INSERT INTO solar_radiation.year_2009 (id, date, solar_radiation, weather_station)
SELECT CONCAT(weather_station, date) AS id, date, solar_radiation, weather_station
FROM source.bom_cleaned
WHERE EXTRACT(YEAR,date) = 2009;

